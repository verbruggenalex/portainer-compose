[
  {
    "type": "container",
    "title": "Drupal",
    "description": "Open-source content management framework",
    "categories": ["CMS"],
    "platform": "linux",
    "logo": "https://cloudinovasi.id/assets/img/logos/drupal.png",
    "image": "drupal:latest",
    "ports": [
      "80/tcp"
    ],
    "volumes": ["/var/www/html"]
  },
  {
    "type": "stack",
    "title": "Horizon2020",
    "description": "Platform site of Horizon2020",
    "note": "Deploys a Wordpress instance connected to a MySQL database.",
    "categories": ["CMS"],
    "platform": "linux",
    "logo": "http://ec.europa.eu/research/images/ec_for_fb.jpg",
    "repository": {
      "url": "https://github.com/verbruggenalex/portainer-compose",
      "stackfile": "templates/stacks/horizon2020.yml"
    },
    "env": [
      {
        "name": "DOCUMENT_ROOT=/var/www/html/build",
        "label": "Document root of site",
        "description": "The location of your built codebase"
      }
    ]
  }
]
