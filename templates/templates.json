[
  {
    "type": "container",
    "title": "fpfis/php71-dev",
    "description": "Open-source HTTP server",
    "categories": ["webserver"],
    "platform": "linux",
    "logo": "https://cloudinovasi.id/assets/img/logos/httpd.png",
    "image": "fpfis/php71-dev",
    "ports": [
      "8080/tcp"
    ],
    "env": [
      {
        "name": "DOCUMENT_ROOT",
        "label": "Document root",
        "set": "/var/www/html/build"
      }
    ],
    "volumes": ["/usr/local/apache2/htdocs/"]
  },
  {
    "type": "container",
    "title": "fpfis/mysql56",
    "description": "The most popular open-source database",
    "categories": ["database"],
    "platform": "linux",
    "logo": "https://cloudinovasi.id/assets/img/logos/mysql.png",
    "image": "fpfis/mysql56",
    "ports": [
      "3306/tcp"
    ],
    "volumes": ["/var/lib/mysql"]
  },
  {
    "type": "stack",
    "title": "Horizon2020",
    "description": "FPFIS Multisite of Horizon2020",
    "note": "Sets up a clone of the production site.",
    "categories": ["website"],
    "platform": "linux",
    "logo": "https://www.rga.eu.com/wp-content/uploads/european-commission.png",
    "repository": {
      "url": "./",
      "stackfile": "stacks/horizon2020.yml"
    },
    "env": [
      {
        "name": "MYSQL_ROOT_PASSWORD",
        "label": "Database root password",
        "description": "Password used by the MySQL root user."
      }
    ],
    "volumes": ["/usr/local/apache2/htdocs/"]
  }
]
